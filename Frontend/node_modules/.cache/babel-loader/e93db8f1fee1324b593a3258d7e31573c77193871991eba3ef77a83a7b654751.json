=\"false\" aria-label=\"Toggle navigation\">\r\n        <span class=\"navbar-toggler-icon\"></span>\r\n      </button>\r\n      <div class=\"collapse navbar-collapse justify-content-end\" :class=\"{ show: isNavbarOpen }\" id=\"navbarSupportedContent\">\r\n        <ul class=\"navbar-nav \">\r\n          <li class=\"nav-item justify-content-end\" >\r\n            <router-link id=\"linkName\" class=\"nav-link active\" aria-current=\"page\" to=\"/\">Home</router-link>\r\n          </li>\r\n          <li class=\"nav-item\">\r\n            <router-link id=\"linkName\" class=\"nav-link active\" aria-current=\"page\" to=\"/about\">About</router-link>\r\n          </li>\r\n          <li class=\"nav-item\">\r\n            <router-link id=\"linkName\" class=\"nav-link active\" aria-current=\"page\" to=\"/admin\">Admin</router-link>\r\n          </li>\r\n          <li class=\"nav-item dropdown\">\r\n            <a class=\"nav-link dropdown-toggle\" href=\"#\" role=\"button\" data-bs-toggle=\"dropdown\" aria-expanded=\"false\">\r\n              Products\r\n            </a>\r\n            <ul class=\"dropdown-menu\">\r\n              <li>\r\n                <router-link class=\"dropdown-item\" to=\"/products\">All</router-link>\r\n              </li>\r\n              <li>\r\n                <router-link class=\"dropdown-item\" to=\"/categories\">Categories</router-link>\r\n              </li>\r\n            </ul>\r\n          </li>\r\n          <li class=\"nav-item\">\r\n            <router-link id=\"linkName\" class=\"nav-link active\" aria-current=\"page\" to=\"/checkout\">Checkout</router-link>\r\n          </li>\r\n          <li class=\"nav-item\">\r\n            <router-link id=\"linkName\" class=\"nav-link active\" aria-current=\"page\" to=\"/contact\">Contact</router-link>\r\n          </li>\r\n        </ul>\r\n        <form class=\"d-flex\" role=\"search\">\r\n          <input class=\"form-control me-2\" type=\"search\" placeholder=\"Search\" aria-label=\"Search\" >\r\n        </form> \r\n\r\n<!-- Button to trigger the modal -->\r\n<button type=\"button\" class=\"btn btn-primary\" data-bs-toggle=\"modal\" data-bs-target=\"#myModal\">\r\n  Login\r\n</button>\r\n\r\n<!-- Modal -->\r\n<div class=\"modal fade\" id=\"myModal\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"myModalLabel\" aria-hidden=\"true\">\r\n  <div class=\"modal-dialog\" role=\"document\">\r\n    <div class=\"modal-content\">\r\n      <!-- Modal content goes here -->\r\n      <div class=\"modal-body\">\r\n        <div class=\"container d-flex p-4\">\r\n          <div class=\"col-1\">\r\n            <img\r\n              src=\"\"\r\n              alt=\"\"\r\n            />\r\n          </div>\r\n          <div class=\"col-6\">\r\n            <h3>WELCOMEðŸ˜Š</h3>\r\n            <form @submit.prevent=\"userLogin\">\r\n              <label for=\"email\" class=\"text-start\">ENTER YOUR EMAIL</label>\r\n              <br />\r\n              <input\r\n                type=\"email\"\r\n                name=\"email\"\r\n                v-model=\"emailAdd\"\r\n                placeholder=\"Enter your email\"\r\n              />\r\n              <br />\r\n              <label for=\"password\" class=\"text-start\">PASSWORD</label>\r\n              <br />\r\n              <input type=\"password\" v-model=\"userPass\" name=\"password\" />\r\n              <button type=\"submit\">Log In</button>\r\n              <p>\r\n                Don't have an account?\r\n                <router-link\r\n                  to=\"/register\"\r\n                  class=\"register-link text-decoration-none text-white\"\r\n                  >Register</router-link\r\n                >\r\n              </p>\r\n            </form>\r\n\r\n            <!-- Sign Up button -->\r\n            <button type=\"button\" class=\"btn btn-secondary\" data-bs-toggle=\"modal\" data-bs-target=\"#signUpModal\">\r\n              Sign Up\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</div>\r\n\r\n<!-- Sign Up Modal -->\r\n<div class=\"modal fade\" id=\"signUpModal\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"signUpModalLabel\" aria-hidden=\"true\">\r\n  <div class=\"container d-flex p-4\">\r\n      <div class=\"col-6\">\r\n        <img\r\n          src=\"https://i.postimg.cc/YCJbkvPZ/c9bl-removebg-preview.png\"\r\n          alt=\"\"\r\n        />\r\n      </div>\r\n      <div class=\"col-6\">\r\n    \r\n  \r\n        <form class=\"row d-flex\" @submit.prevent=\"register\">\r\n          <div class=\"col-6\">\r\n            <label for=\"firstName\" class=\"text-start\">First Name:</label>\r\n            <input\r\n              type=\"text\"\r\n              placeholder=\"Enter your name\"\r\n              v-model=\"firstName\"\r\n              required\r\n            />\r\n            <label for=\"lastname\" class=\"text-start\">Surname:</label>\r\n            <input\r\n              type=\"text\"\r\n              placeholder=\"Enter your surname\"\r\n              v-model=\"lastName\"\r\n              required\r\n            />\r\n            <label for=\"role\" class=\"text-start\">Role:</label>\r\n            <input\r\n              type=\"text\"\r\n              placeholder=\"Enter your role\"\r\n              v-model=\"userRole\"\r\n              required\r\n            />\r\n            <label for=\"text\" class=\"text-start\">userUrl</label>\r\n  \r\n            <input\r\n              type=\"text\"\r\n              placeholder=\"\"\r\n              v-model=\"userProfile\"\r\n              required\r\n            />\r\n          </div>\r\n          <div class=\"col-6\">\r\n            <label for=\"gender\" class=\"text-start\">Gender:</label>\r\n            <input\r\n              type=\"gender\"\r\n              placeholder=\"Enter you gender\"\r\n              v-model=\"gender\"\r\n              required\r\n            />\r\n            <label for=\"email\" class=\"text-start\">EMAIL:</label>\r\n  \r\n            <input\r\n              type=\"email\"\r\n              placeholder=\"Enter your email\"\r\n              v-model=\"emailAdd\"\r\n              required\r\n            />\r\n  \r\n            <label for=\"age\" class=\"text-start\">Age:</label>\r\n            <input\r\n              type=\"number\"\r\n              placeholder=\"Enter your age\"\r\n              v-model=\"userAge\"\r\n              required\r\n            />\r\n            <label for=\"password\" class=\"text-start\">PASSWORD</label>\r\n  \r\n            <input type=\"password\" v-model=\"userPass\" required />\r\n          </div>\r\n          <button class=\"submit\">Sign Up</button>\r\n        </form>\r\n  \r\n      </div>\r\n    </div>\r\n</div>\r\n\r\n\r\n</div>\r\n</div>\r\n</nav>\r\n\r\n\r\n  \r\n</template>\r\n\r\n<script>\r\n\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      emailAdd: \"\",\r\n      userPass: \"\",\r\n    };\r\n  },\r\n  beforeCreate() {\r\n    this.$store.dispatch(\"cookieCheck\");\r\n  },\r\n  methods: {\r\n    async userLogin() {\r\n      console.log(\"Reached\");\r\n      try {\r\n        const payload = {\r\n          emailAdd: this.emailAdd,\r\n          userPass: this.userPass,\r\n        };\r\n        const resp = await this.$store.dispatch(\"login\", payload);\r\n        console.log(\"Login Response:\", resp);\r\n        if (resp.success && resp.token) {\r\n          await Swal.fire({\r\n            icon: \"success\",\r\n            title: \"Logged in Successfully\",\r\n            text: \"You are now logged in!\",\r\n          });\r\n          this.$router.push(\"/\");\r\n        } else {\r\n          const errMsg = resp.error || \"Unexpected error\";\r\n          await Swal.fire({\r\n            icon: \"error\",\r\n            title: \"Login failed\",\r\n            text: errMsg,\r\n          });\r\n        }\r\n      } catch (e) {\r\n        console.error(\"Error while logging in: \", e);\r\n      }\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    openLoginModal() {\r\n      // Toggle the Bootstrap modal using jQuery\r\n      $('#myModal').modal('show');\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\nbody{\r\n  background-color: rgb(238, 216, 248);\r\n}\r\n\r\n.navbar-nav, .form{\r\nbackground-color: white;\r\ncolor:white;\r\nwidth: fit-content;\r\n\r\n}\r\n\r\n.nav-link{\r\n  color: white;\r\n}\r\n\r\n.nav-link:hover{\r\n  background-color: purple;\r\n  border-radius: 50%;\r\n  color: white;\r\n \r\n}\r\n\r\n.btn {\r\n  border:white;\r\n  margin: white;\r\n  outline: white;\r\n  color:white;\r\n  border: 3px solid purple;\r\n}\r\n</style>\r\n"],"mappings":";AA4LA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,YAAYA,CAAA,EAAG;IACb,IAAI,CAACC,MAAM,CAACC,QAAQ,CAAC,aAAa,CAAC;EACrC,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,SAASA,CAAA,EAAG;MAChBC,OAAO,CAACC,GAAG,CAAC,SAAS,CAAC;MACtB,IAAI;QACF,MAAMC,OAAM,GAAI;UACdT,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC;QACD,MAAMS,IAAG,GAAI,MAAM,IAAI,CAACP,MAAM,CAACC,QAAQ,CAAC,OAAO,EAAEK,OAAO,CAAC;QACzDF,OAAO,CAACC,GAAG,CAAC,iBAAiB,EAAEE,IAAI,CAAC;QACpC,IAAIA,IAAI,CAACC,OAAM,IAAKD,IAAI,CAACE,KAAK,EAAE;UAC9B,MAAMC,IAAI,CAACC,IAAI,CAAC;YACdC,IAAI,EAAE,SAAS;YACfC,KAAK,EAAE,wBAAwB;YAC/BC,IAAI,EAAE;UACR,CAAC,CAAC;UACF,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,GAAG,CAAC;QACxB,OAAO;UACL,MAAMC,MAAK,GAAIV,IAAI,CAACW,KAAI,IAAK,kBAAkB;UAC/C,MAAMR,IAAI,CAACC,IAAI,CAAC;YACdC,IAAI,EAAE,OAAO;YACbC,KAAK,EAAE,cAAc;YACrBC,IAAI,EAAEG;UACR,CAAC,CAAC;QACJ;MACF,EAAE,OAAOE,CAAC,EAAE;QACVf,OAAO,CAACc,KAAK,CAAC,0BAA0B,EAAEC,CAAC,CAAC;MAC9C;IACF;EACF,CAAC;EAEDjB,OAAO,EAAE;IACPkB,cAAcA,CAAA,EAAG;MACf;MACAC,CAAC,CAAC,UAAU,CAAC,CAACC,KAAK,CAAC,MAAM,CAAC;IAC7B;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}